    <%= render 'layouts/shim' %>
  <link rel="stylesheet" type="text/css" href="//wpcc.io/lib/1.0.2/cookieconsent.min.css"/><script src="//wpcc.io/lib/1.0.2/cookieconsent.min.js"></script><script>window.addEventListener("load", function(){window.wpcc.init({"border":"thick","colors":{"popup":{"background":"#f9f9f9","text":"#006994","border":"#006994"},"button":{"background":"#006994","text":"#f9f9f9"}},"position":"bottom-right","corners":"normal","content":{"href":"localhost:3000/privacy_notice"}})});</script>
  <link href="favicon.png" rel="icon">  
  <script src="https://kit.fontawesome.com/44e42f36cb.js" crossorigin="anonymous"></script>
  <%= favicon_link_tag asset_path('quotes_logo_img.ico') %>
  </head>

 <body>
    <%= render 'layouts/header' %>
      <%= yield %>
    <%= render 'layouts/footer' %>
